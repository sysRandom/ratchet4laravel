{
    "name": "sysrandom/ratchet4laravel",
    "description": "A Ratchet Server built for Laravel",
    "keywords": ["laravel"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.2.0",
        "ext-zmq": "*",
        "cboden/ratchet": "^0.4.3",
        "graham-campbell/throttle": "^7.4|^8.1",
        "illuminate/console": "~6.0|~7.0|~8.0",
        "illuminate/support": "~6.0|~7.0|~8.0",
        "react/zmq": "0.2.*|0.3.*|0.4.*"
    },
    "autoload": {
        "psr-4": {
            "Askedio\\LaravelRatchet\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Askedio\\LaravelRatchet\\Providers\\LaravelRatchetServiceProvider"
            ]
        }
    }
}
